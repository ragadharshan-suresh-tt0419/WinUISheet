using System;
using System.Collections.Generic;
using System.Linq;
using System.Text;
using System.Threading.Tasks;

namespace gridLevel2LL
{
    internal class Row
    {
        private int rowIdx;
        private Cell head;
        private Row next;

        public Row(int r)
        {
            this.head = null;
            this.next = null;
            this.rowIdx = r;
        }

        public int getRowIdx()
        {
            return rowIdx;
        }
    }
}
