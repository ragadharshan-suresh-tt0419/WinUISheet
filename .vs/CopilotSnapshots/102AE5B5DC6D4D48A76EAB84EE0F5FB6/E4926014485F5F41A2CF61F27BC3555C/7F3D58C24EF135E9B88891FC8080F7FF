using System;
using System.Collections.Generic;
using System.Linq;
using System.Text;
using System.Threading.Tasks;

namespace gridLevel2LL
{
    internal class Cell
    {
        private int rowIdx;
        private int colIdx;
        private string value;

        Cell next;

        public Cell(int rowId, int colId, string value)
        {
            this.rowIdx = rowId;
            this.colIdx = colId;
            this.value = value;
            this.next = null;
        }


    }
}
